    "react-native-svg": "^9.13.6",
    "react-native-swipeout": "^2.3.6",
    "react-native-system-setting": "^1.7.3",
    "react-native-tab-view": "^2.5.0",
    "react-native-tailwindcss": "^1.1.11",
