    "css-loader": "^6.4.0",
    "eslint": "7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "style-loader": "^3.3.0",
    "tailwindcss": "^2.2.16"
  },
  "jest": {
    "preset": "react-native"
  }
